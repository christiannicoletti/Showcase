{
  "expo": {
    "name": "Creatist",
    "slug": "Creatist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/creatist_icon.png",
    "splash": {
      "image": "./assets/creatist_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.creatist.ios"
    },
    "web": {
      "favicon": "./assets/creatist_icon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "svgx"
      ]
    }
  }
}
